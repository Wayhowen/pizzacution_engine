    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="NoSameToppingsDefinedTwice" value="Tuple {&#xA;&#x9;message : String = name + ' should not be defined twice',&#xA;&#x9;status : Boolean = &#xA;&#x9;&#x9;&#x9;self.oclContainer.oclAsType(PizzaPlace).toppingsAvailable -> &#xA;&#x9;&#x9;&#x9;&#x9;select(t | t &lt;> self) ->&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; forAll(topping | self.name &lt;> topping.name)&#xA;}.status"/>
