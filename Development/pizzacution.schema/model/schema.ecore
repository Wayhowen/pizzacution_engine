  <eClassifiers xsi:type="ecore:EClass" name="Size" eSuperTypes="#//NamedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="NoSameSizeDefinedTwice NoTwoSizeIsServesTheSameAmountOfPpl NoTwoSizePriceMultipliersAreTheSame"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="NoSameSizeDefinedTwice" value="Tuple {&#xA;&#x9;message : String = name + ' should not be defined twice',&#xA;&#x9;status : Boolean = &#xA;&#x9;&#x9;&#x9;self.oclContainer.oclAsType(PizzaPlace).sizesAvailable -> &#xA;&#x9;&#x9;&#x9;&#x9;select(s | s &lt;> self) ->&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; forAll(size | self.name &lt;> size.name)&#xA;}.status"/>
      <details key="NoTwoSizeIsServesTheSameAmountOfPpl" value="Tuple {&#xA;&#x9;message : String = name + ' should not serve the same amount of people as any other size',&#xA;&#x9;status : Boolean = &#xA;&#x9;&#x9;&#x9;self.oclContainer.oclAsType(PizzaPlace).sizesAvailable -> &#xA;&#x9;&#x9;&#x9;&#x9;select(s | s &lt;> self) ->&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; forAll(size | self.serves &lt;> size.serves)&#xA;}.status"/>
      <details key="NoTwoSizePriceMultipliersAreTheSame" value="Tuple {&#xA;&#x9;message : String = name + ' should not multiply the price the same way as any other size',&#xA;&#x9;status : Boolean = &#xA;&#x9;&#x9;&#x9;self.oclContainer.oclAsType(PizzaPlace).sizesAvailable -> &#xA;&#x9;&#x9;&#x9;&#x9;select(s | s &lt;> self) ->&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; forAll(size | self.priceMultiplier &lt;> size.priceMultiplier)&#xA;}.status"/>
    </eAnnotations>
