grammar org.mdse.minisql.types.MSQLTypes with org.eclipse.xtext.common.Terminals

import "http://mdse.org/minisql/types" 
import "http://www.eclipse.org/emf/2002/Ecore" as ecore

Type:
	IntegerType | StringType | BooleanType
;

IntegerType:
	"Int" {IntegerType}
;

StringType:
	"String" {StringType}
;

BooleanType:
	"Bool" {BooleanType}
;