@namespace(uri="http://mdse.org/minisql/schema", prefix="minisqlschema")
package schema;

import "platform:/resource/org.mdse.minisql.types/model/MiniSQLTypes.ecore"; 

abstract class NamedElement {
	attr String[1] name;
}

class Table extends NamedElement {
