<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="schema" nsURI="http://www.example.org/schema" nsPrefix="schema">
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Pizza" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="toppings" upperBound="-1"
        eType="#//Topping"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="price" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="newAttribute"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="sauce" unique="false" eType="#//Sauces%20%2F%20Bases"
        defaultValueLiteral="tmt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dough" eType="#//Dough%20Types"
        defaultValueLiteral="nys"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Topping" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pizza" upperBound="-1"
        eType="#//Pizza"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Dough Types">
    <eLiterals name="New Haven Style" literal="nhs"/>
    <eLiterals name="St. Louis Style" value="1" literal="stls"/>
    <eLiterals name="Neapolitan" value="2" literal="npn"/>
    <eLiterals name="New York Style" value="3" literal="nys"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Sauces / Bases">
    <eLiterals name="BBQ" literal="bbq"/>
    <eLiterals name="Tomato" value="1" literal="tmt"/>
    <eLiterals name="Garlic" value="2" literal="glc"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Recipe" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="steps" lowerBound="1" upperBound="-1"
        eType="#//Step"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Step">
    <eStructuralFeatures xsi:type="ecore:EReference" name="recipe" lowerBound="1"
        eType="#//Recipe"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Nutritions" abstract="true"/>
</ecore:EPackage>
